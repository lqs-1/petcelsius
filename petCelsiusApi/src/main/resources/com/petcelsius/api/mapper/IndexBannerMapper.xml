<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.petcelsius.api.mapper.IndexBannerMapper">
    <!-- SQL语句 -->
<!--    首页的banner获取，索引在前五的-->
    <select id="selectBannerToFive" resultType="com.petcelsius.api.domain.IndexBanner">
        select * from index_banner order by `index` asc limit 5
    </select>





</mapper>