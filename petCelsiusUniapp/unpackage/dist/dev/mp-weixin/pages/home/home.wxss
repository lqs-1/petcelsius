page {
  background-color: #ccffff;
}
swiper {
  height: 340rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin: 30rpx 20rpx;
  background-color: #ffffcc;
}
.nav-list .nav-item {
  margin: 30rpx;
  text-align: center;
}
.nav-list .nav-item .nav-img {
  justify-content: center;
  width: 100rpx;
  height: 100rpx;
}
.nav-list .nav-item .nav-title {
  text-align: center;
  color: #333;
  font-size: 30rpx;
}
.floor-title {
  margin: 15rpx 0;
  width: 100%;
  height: 60rpx;
}
.floor_index_img {
  width: 232rpx;
}
.right-img-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.right-img-box .floor_img {
  width: 233rpx;
}
.floor-img-box {
  display: flex;
}

